{
  "name": "astro-ui-library",
  "homepage": "https://jordanurbaezlu.github.io/astro-ui-library",
  "version": "0.0.0-development",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.56",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-scripts": "5.0.1",
    "sass": "^1.54.8",
    "typescript": "<4.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx,css,scss}\"",
    "build": "react-scripts build",
    "commit": "git cz",
    "test": "react-scripts test --watchAll=false",
    "test-u": "react-scripts test -u --watchAll=false",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "clean": "rm -r coverage build storybook-static",
    "semantic-release": "semantic-release --branches main"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-interactions": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/node-logger": "^6.5.10",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.10",
    "@storybook/testing-library": "^0.0.13",
    "@types/jest-axe": "^3.5.4",
    "babel-plugin-named-exports-order": "^0.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "gh-pages": "^4.0.0",
    "jest-axe": "^6.0.0",
    "prettier": "^2.2.1",
    "prop-types": "^15.8.1",
    "semantic-release": "^19.0.5",
    "storybook-dark-mode": "^1.1.2",
    "webpack": "^5.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JordanUrbaezLu/astro-ui-library.git"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
